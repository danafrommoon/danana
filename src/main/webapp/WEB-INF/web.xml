<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <display-name>Session Management</display-name>

    <servlet>
        <description>Session Management</description>
        <display-name>session</display-name>
        <servlet-name>Session Management</servlet-name>
        <servlet-class>app.map.tap.SessionManagement</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Session Management</servlet-name>
        <url-pattern>/sessionmanagement</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>Controller</welcome-file>
    </welcome-file-list>
    <servlet>
        <description>Controller for every part</description>
        <display-name>Controller</display-name>
        <servlet-name>Controller</servlet-name>
        <servlet-class>controller.Controller</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Controller</servlet-name>
        <url-pattern>/Controller</url-pattern>
    </servlet-mapping>
</web-app>